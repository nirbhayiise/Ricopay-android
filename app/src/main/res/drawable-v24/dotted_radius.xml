<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">

        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#4CAF50"
                android:dashGap="3dp"
                android:dashWidth="1dp" />

            <solid android:color="@android:color/transparent" />


        </shape>
    </item>
</layer-list>